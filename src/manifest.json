{
  "name": "Google PageSpeed Insights",
  "description": "Open current page in Google PageSpeed Insights",
  "version": "1.0",
  "page_action": {
    "default_icon": "icon_208.png"
  },
  "icons": {
    "16": "icon_208.png",
    "48": "icon_208.png",
    "128": "icon_208.png"
  },
  "background": {
    "scripts": [
      "main.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "manifest_version": 2
}